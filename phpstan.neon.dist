includes:
    - phpstan-baseline.neon

parameters:

    paths:
        - src
        - tests

    # The level 9 is the highest level
    level: 9

    ignoreErrors:

    excludePaths:
        - ./*/*/FileToBeExcluded.php
        - ./dir/dir/FileToBeExcluded.php

    editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'

    checkMissingIterableValueType: false